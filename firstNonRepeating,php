<?php
function findNonRepeatingChar($str)
{
    //Count string
    $count = 0;
    while(isset($str[$count])){
        $count++;
    }   
    for($i=0;$i<=$count;$i++){

    }
}
$string = "Rajesh";
echo "======= Output =======</br>";
echo "Original String:" . $string . "</br>";
$firstNonRepeatingChar = findNonRepeatingChar($string);
echo "First Non Repeating Char:" . $firstNonRepeatingChar;
